    <extensionMap>
      <mapping pattern="LICENSE" type="AUTO_DETECTED" />
      <mapping pattern=".env.*" type="Env File" />
      <mapping ext="env" type="Env File" />
      <mapping ext="gz" type="Native" />
      <mapping ext="lnk" type="PLAIN_TEXT" />
      <mapping ext="po" type="PLAIN_TEXT" />
      <removed_mapping ext="ignore" approved="true" type="IgnoreLang file" />
      <removed_mapping pattern=".env.dev" approved="true" type="PLAIN_TEXT" />
    </extensionMap>
  </component>
</application>